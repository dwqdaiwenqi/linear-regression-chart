{
  "compilerOptions": {
      "target": "es2020",
      "module": "esnext",
      "moduleResolution": "node",
      "outDir": "./lib",
      "strict": true,
      "esModuleInterop": true,
      "sourceMap": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "resolveJsonModule": true
  },
  "include": [
      "./src"
  ]
}
